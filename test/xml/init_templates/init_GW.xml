<init name        = 'test_name'
      description = 'test_description'>
  <test file      = 'info.xml'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
	  ignore    = 'timing;rmslog10'
	  tolIter   = '3'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '3'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1e-3'
          tolMSE    = '1e-3'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EFERMI_GW.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EVALQP.DAT'
          tolFloat  = '0.1'
          tolMSE    = '0.1'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'VXCNN.DAT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EPS00_GW.OUT'
          tolFloat  = '0.1'
          tolMSE    = '0.1'
          condition = '0 0'
          ignore    = ''/>
</init>
