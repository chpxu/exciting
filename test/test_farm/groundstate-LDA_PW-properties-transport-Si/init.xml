<init name        = 'groundstate-LDA_PW-properties-transport-Si'
      description = 'The groundstate and transport properties of Si are tested.'>
  <test file      = 'info.xml'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
	  ignore    = 'timing;rmslog10'
	  tolIter   = '3'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '3'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1e-3'
          tolMSE    = '1e-3'
          condition = '0 0'
          ignore    = ''>
          <eigval
              eigvalPair = '2,3 5,8 8,9 5,6 1,9'
              tolEigval   = '1e-8'/>
        </test>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'SEEBECK_11.OUT'
          tolFloat  = '1'
          tolMSE    = '1'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'ELECTCOND_11.OUT'
          tolFloat  = '1'
          tolMSE    = '4e12'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'THERMALCOND_11.OUT'
          tolFloat  = '1'
          tolMSE    = '4e9'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'Z_11.OUT'
          tolFloat  = '1'
          tolMSE    = '1'
          condition = '0 0'
          ignore    = ''/>
</init>
