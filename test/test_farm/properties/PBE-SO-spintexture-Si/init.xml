<init name        = 'groundstate-GGA_PBE-spinorbitcoupling-spintexture-Si'
      description = 'All groundstate file are tested.'>
  <test file      = 'info.xml'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
	  ignore    = 'timing;rmslog10'
	  tolIter   = '5'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '5'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1'
          tolMSE    = '1'
          condition = '0 0'
          ignore    = ''>
	  <eigval
	      eigvalPair = '4,5 5,8 8,9'
	      tolEigval   = '1e-8'/>
	</test>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'spintext.xml'
          tolFloat  = '5e-1'
          tolMSE    = '5e-1'
          condition = '0 0'
          ignore    = ''>
	  </test>
</init>
