<init name        = 'groundstate_GGA_PBE_electronicstructure_Al'
      description = 'Mixer msec is invoked.'>
  <test   file      = 'info.xml'
          tolFloat  = '1e-4'
          tolMSE    = '1e-4'
          condition = '0 0'
	  ignore    = 'timing rmslog10'
	  tolIter   = '3'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-4'
          tolMSE    = '1e-4'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '3'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1e-3'
          tolMSE    = '1e-3'
          condition = '0 0'
          ignore    = ''>
          <eigval
              eigvalPair = '1,2 6,7 4,5 5,8 9,10'
              tolEigval   = '1e-8'/>
        </test>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
</init>
