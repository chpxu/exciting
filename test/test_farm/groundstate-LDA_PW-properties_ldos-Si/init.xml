<init name        = 'groundstate-LDA_PW-properties_ldos-Si'
      description = 'All groundstate files are tested.'>
  <test file      = 'info.xml'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
	  ignore    = 'timing;rmslog10'
	  tolIter   = '3'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '3'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1e-3'
          tolMSE    = '1e-3'
          condition = '0 0'
          ignore    = ''>
     <eigval
              eigvalPair = '2,3 5,6 7,8 9,10 1,7'
              tolEigval   = '1e-8'/>
      </test>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'ldos.out'
          tolFloat  = '1'
          tolMSE    = '1'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'band_edges.out'
          tolFloat  = '0.1'
          tolMSE    = '0.1'
          condition = '0 0'
          ignore    = ''/>


</init>
