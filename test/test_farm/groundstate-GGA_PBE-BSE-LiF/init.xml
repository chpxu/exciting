<init name        = 'groundstate_GGA_PBE_BSE_LiF'
      description = 'All groundstate and BSE files are tested.'
      executable  = 'excitingser'>
  <test file      = 'info.xml'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
	  ignore    = 'timing;rmslog10'
	  tolIter   = '3'/>
  <test   file      = 'INFO.OUT'
          tolFloat  = '1e-6'
          tolMSE    = '1e-6'
          condition = '0 0'
          ignore    = 'Wall time (seconds);CPU time for scf_cycle (seconds);CPU time for vxnl (seconds)'
          tolIter   = '3'/>
  <test   file      = 'atoms.xml'          
          tolFloat  = '1e-6'              
          tolMSE    = '1e-6'              
          condition = '0 0'               
          ignore    = ''/>
  <test   file      = 'eigval.xml'
          tolFloat  = '1e-3'
          tolMSE    = '1e-3'
          condition = '0 0'
          ignore    = ''>
          <eigval
              eigvalPair = '4,5 5,8 8,9 33,34 49,50 3,120'
              tolEigval   = '1e-8'/>
        </test>
  <test   file      = 'evalcore.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'geometry.xml'
          tolFloat  = '1e-8'
          tolMSE    = '1e-8'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EPSILON/EPSILON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC11.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EPSILON/EPSILON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC22.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EPSILON/EPSILON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC33.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EXCITON/EXCITON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC11.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EXCITON/EXCITON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC22.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  <test   file      = 'EXCITON/EXCITON_NAR_BSE-singlet-TDA-BAR_SCR-full_OC33.OUT'
          tolFloat  = '1e-7'
          tolMSE    = '1e-7'
          condition = '0 0'
          ignore    = ''/>
  
</init>
