#%Module
module-whatis    "Setup intel compiler suite version 2019 for SOL machines using physics NFS server"
conflict intel
setenv NLSPATH {/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/compiler/lib/intel64/locale/%l_%t/%N:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/mkl/lib/intel64_lin/locale/%l_%t/%N:/usr/global/intel-2019/debugger_2019/gdb/intel64/share/locale/%l_%t/%N};
