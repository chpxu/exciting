setenv TBBROOT {/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/tbb};
prepend-path PATH {/usr/global/intel-2019/debugger_2019/gdb/intel64/bin};
prepend-path PATH {/usr/global/intel-2019//compilers_and_libraries_2019.0.117/linux/mpi/intel64/bin};
