setenv I_MPI_ROOT {/usr/global/intel-2019//compilers_and_libraries_2019.0.117/linux/mpi};
setenv INTEL_LICENSE_FILE {2500@liz.cms.hu-berlin.de};
setenv CPATH {/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/ipp/include:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/mkl/include:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/pstl/include:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/tbb/include:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/tbb/include:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/daal/include};
setenv LD_LIBRARY_PATH {/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/compiler/lib/intel64_lin:/usr/global/intel-2019//compilers_and_libraries_2019.0.117/linux/mpi/intel64/lib/release:/usr/global/intel-2019//compilers_and_libraries_2019.0.117/linux/mpi/intel64/lib:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/ipp/lib/intel64:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/compiler/lib/intel64_lin:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/mkl/lib/intel64_lin:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/tbb/lib/intel64/gcc4.7:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/tbb/lib/intel64/gcc4.7:/usr/global/intel-2019/debugger_2019/libipt/intel64/lib:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/daal/lib/intel64_lin:/usr/global/intel-2019/compilers_and_libraries_2019.0.117/linux/daal/../tbb/lib/intel64_lin/gcc4.4};
